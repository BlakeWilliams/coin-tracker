{
  "name": "coin-tracker",
  "version": "0.0.2",
  "description": "Track virtual dollars for virtual profit",
  "author": "Blake Williams",
  "license": "MIT",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "precommit": "lint-staged",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "release": "build"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "electron-builder": "^19.49.4",
    "electron-prebuilt-compile": "1.8.2-beta.3",
    "lodash": "^4.17.4",
    "systemjs": "^0.20.19"
  },
  "dependencies": {
    "@types/lodash": "^4.14.91",
    "classnames": "^2.2.5",
    "electron-compile": "^6.4.2",
    "electron-is-dev": "^0.3.0",
    "electron-redux": "^1.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^1.3.0",
    "electron-updater": "^2.17.8",
    "font-awesome": "^4.7.0",
    "format-currency": "^1.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0-beta.9",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "typescript": "^2.6.2"
  },
  "build": {
    "appId": "me.blakewilliams.coin-tracker",
    "publish": {
      "provider": ["github"],
      "owner": "BlakeWilliams"
    },
    "mac": {
      "icon": "app/images/mac.icns",
      "target": "zip"
    }
  }
}
