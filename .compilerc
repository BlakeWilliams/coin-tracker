{
  "application/javascript": {
    "presets": [
      "latest",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "sourceMaps": "inline"
  },
  "text/typescript": {
    "babel": {
      "presets": [
        "latest",
        "react"
      ],
      "sourceMaps": "both"
    },
    "lib": [
      "dom",
      "es7"
    ],
    "jsx": "react",
    "module": "es6",
    "sourceMap": true
  }
}
